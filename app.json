{
  "expo": {
    "name": "face-iaos",
    "slug": "face-iaos",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "packagerOpts": {
      "port": 56954
    },
    "ios": {
      "bundleIdentifier": "com.way2bit.astelliaroyal",
      "buildNumber": "2.0.0",
      "supportsTablet": true
    },
    "android": {
      "package": "com.way2bit.astelliaroyal",
      "versionCode": 3,
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE"
      ],
         "config": {
        "googleMobileAdsAppId": "ca-app-pub-5747609340870218~4551553792" // sample id, replace with your own
      }
    }
  }
}
